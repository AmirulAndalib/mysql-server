-- vertical_results

SELECT
event_name,
work_completed > 0,
work_estimated > 0,
work_completed <= work_estimated
FROM performance_schema.events_stages_history_long
WHERE event_name LIKE 'stage/innodb/alter table%'
ORDER BY 1;

DROP TABLE wl5889;
