==== Configure ====
CREATE FUNCTION fix_gtid(str VARCHAR(100)) RETURNS VARCHAR (100)
RETURN REPLACE(REPLACE(IFNULL(str, 'NULL'),
@@GLOBAL.SERVER_UUID, 'SERVER_UUID'),
'aaaaaaaa-aaaa-aaaa-aaaa-aaaaaaaaaaaa', 'A')|
CREATE FUNCTION show_gtid() RETURNS VARCHAR(100) BEGIN
SET @history_gtid= 'none';
SET @current_gtid= 'none';
SELECT fix_gtid(GTID) FROM performance_schema.events_transactions_history
INTO @history_gtid;
SELECT fix_gtid(GTID) FROM performance_schema.events_transactions_current
INTO @current_gtid;
RETURN CONCAT("- history=", @history_gtid, " current=", @current_gtid);
END|
CREATE TABLE t1 (a INT);
