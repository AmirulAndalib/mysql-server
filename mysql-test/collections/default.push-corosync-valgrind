# Run the rpl test suite with GCS replication
perl mysql-test-run.pl --force --timer --debug-server --parallel=auto --experimental=collections/default.experimental --comment=rpl_gcs-valgrind --vardir=var-rpl_gcs_valgrind --suite=rpl --mysqld=--enforce-gtid-consistency --mysqld=--log-slave-updates --mysqld=--gtid-mode=on --skip-test-list=collections/disabled-gtid-on.list --skip-test-list=collections/disabled-per-push.list --mysqld=--binlog-format=row --mysqld=--binlog-checksum=NONE --mysqld=--plugin-load=gcs_replication_plugin.so --mysqld=--plugin-dir=lib/plugin/debug/
