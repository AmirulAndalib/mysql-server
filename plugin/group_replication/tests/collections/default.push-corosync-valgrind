##################
# Valgrind build #
##################

# Group Replication tests (on plugin)
perl mysql-test-run.pl --force --timer --debug-server --parallel=auto --experimental=collections/default.experimental --comment=group_replication-valgrind --vardir=var-group_replication-valgrind --suite=group_replication --mysqld=--enforce-gtid-consistency --mysqld=--log-slave-updates --mysqld=--gtid-mode=on --mysqld=--binlog-format=row --mysqld=--binlog-checksum=NONE --mysqld=--master-info-repository=TABLE --mysqld=--relay-log-info-repository=TABLE --mysqld=--transaction-write-set-extraction=MURMUR32 --mysqld=--plugin-load=group_replication.so --mysqld=--plugin-dir=lib/plugin/debug/ --big-test --testcase-timeout=60 --suite-timeout=360

# Already existent tests on rpl suite that support Group Replication
perl mysql-test-run.pl --force --timer --debug-server --parallel=auto --experimental=collections/default.experimental --comment=rpl_group_replication-valgrind --vardir=var-rpl_group_replication-valgrind --suite=rpl --mysqld=--enforce-gtid-consistency --mysqld=--log-slave-updates --mysqld=--gtid-mode=on --skip-test-list=collections/disabled-gtid-on.list --mysqld=--binlog-format=row --mysqld=--binlog-checksum=NONE --mysqld=--master-info-repository=TABLE --mysqld=--relay-log-info-repository=TABLE --mysqld=--transaction-write-set-extraction=MURMUR32 --mysqld=--plugin-load=group_replication.so --mysqld=--plugin-dir=lib/plugin/debug/ --big-test --testcase-timeout=60 --suite-timeout=360
