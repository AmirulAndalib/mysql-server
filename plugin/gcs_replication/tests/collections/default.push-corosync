#################
# Release build #
#################

# Group Replication tests (on plugin)
perl mysql-test-run.pl --force --timer --parallel=auto --experimental=collections/default.experimental --comment=gcs_replication --vardir=var-gcs_replication --suite=gcs_replication --mysqld=--enforce-gtid-consistency --mysqld=--log-slave-updates --mysqld=--gtid-mode=on --mysqld=--binlog-format=row --mysqld=--transaction-write-set-extraction=MURMUR32 --mysqld=--binlog-checksum=NONE --mysqld=--plugin-load=gcs_replication_plugin.so --mysqld=--plugin-dir=lib/plugin/ --big-test --testcase-timeout=60 --suite-timeout=360

# Already existent tests on rpl suite that support Group Replication
perl mysql-test-run.pl --force --timer --parallel=auto --experimental=collections/default.experimental --comment=rpl_gcs --vardir=var-rpl_gcs --suite=rpl --mysqld=--enforce-gtid-consistency --mysqld=--log-slave-updates --mysqld=--gtid-mode=on --skip-test-list=collections/disabled-gtid-on.list --mysqld=--binlog-format=row --mysqld=--transaction-write-set-extraction=MURMUR32 --mysqld=--binlog-checksum=NONE --mysqld=--plugin-load=gcs_replication_plugin.so --mysqld=--plugin-dir=lib/plugin/ --big-test --testcase-timeout=60 --suite-timeout=360


